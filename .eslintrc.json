{
  "extends": ["airbnb-base", "prettier"],
  "env": {
    "browser": true,
    "jquery": true
  },
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "globals": {
    "BarcodeApp": true
  },
  "rules": {
    "no-console": "off",
    "no-alert": "off",
    "func-names": "off",
    "no-use-before-define": ["error", { "functions": false, "classes": true }],
    "consistent-return": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  }
}
